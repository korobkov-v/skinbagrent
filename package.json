{
  "name": "skinbag-mcp",
  "version": "0.1.1",
  "private": false,
  "description": "Open-source Skinbag.rent style MCP + API marketplace for AI agents to book humans",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "skinbag-mcp-server": "bin/mcp-server.cjs"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:mcp": "tsx src/mcpServer.ts",
    "clean": "node scripts/clean-workspace.cjs",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && npm run test:smoke",
    "test:smoke": "node --test tests/smoke.test.mjs",
    "test:e2e:http": "npm run build && node --test tests/e2e-http.test.mjs",
    "test:e2e:ui": "npm run build && node --test tests/e2e-ui.test.mjs",
    "start": "node dist/server.js",
    "start:mcp": "node dist/mcpServer.js",
    "prepublishOnly": "npm run build",
    "migrate:sqlite-to-postgres": "node scripts/migrate-sqlite-to-postgres.cjs"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@types/pg": "^8.16.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
